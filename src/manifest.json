{
  "name": "Abstract Fix",
  "author": "Goncalo Espinha",
  "authorEmail": "gonespinha@gmail.com",
  "version": "0.1.0",
  "homepage": "https://github.com/gespinha/sketch-abstract-fix",
  "icon": "icon.png",
  "description": "A plugin to fix Abstract symbol links",
  "identifier": "com.abstract-fix.ge",
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "commands": [
    {
      "script": "./script.js",
      "name": "Fix Symbols",
      "identifier": "symbol-fix",
      "handler": "processFix"
    },
    {
      "script": "./script.js",
      "name": "Fix and Detach Symbols",
      "identifier": "symbol-detach",
      "handler": "processDetach"
    }
  ],
  "menu": {
    "title": "Abstract Fix",
    "items": [
      "symbol-fix",
      "symbol-detach"
    ]
  }
}
